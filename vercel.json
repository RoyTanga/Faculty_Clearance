{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "app.py"
      }
    ]
  }


{
  "version": 2,
  "builds": [
    { "src": "webpack.config.js", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

  
